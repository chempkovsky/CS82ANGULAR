        [Display(Description = "Role Id", Name = "Role Id", Prompt = "Enter Id", ShortName = "Id")]
        [DatabaseGenerated(DatabaseGeneratedOption.Computed)]
        [StringLength(128, MinimumLength = 1, ErrorMessage = "Invalid")]
