  .stickyfirstclss {
    position:sticky;
    left:0;
    z-index:1;
    background-color: white;  
    
  }
  .stickylastclss {
    position:sticky;
    right:0;
    z-index:1;
    background-color: white; 
  }
  .stickytopclss {
    white-space: nowrap;
    position:sticky;
    background-color: white;
    z-index:1;
    top:0;
  }
  .stickyfirsttopclss {
    position:sticky;
    background-color: white;
    z-index: 2;
    top:0;
    left:0;
    width:20px;
  }
  .stickylasttopclss {
    position:sticky;
    background-color: white;
    z-index: 2;
    top:0;
    right:0;
    width:20px;
  }
  .tblwidthclss {
    width:100%;
  }
  .dropdown-toggle::after{
    display:none; 
  } 
  .ngb-tbl-outer-container {
    display: flex; 
    flex-wrap: wrap;
  }
 .ngb-tbl-container {
    flex:1;
    overflow-x: auto;
    min-width: 0px;
 }
.pagination-container {
    display:flex;
    flex-direction:row;
    justify-content: space-between;
    padding-top: 5px;
}

 th[sorthead] {
  cursor: pointer;
  user-select: none;
  -webkit-user-select: none; 
}
th[sorthead].sortheaddesc:before, th[sorthead].sortheadasc:before {
  content: '';
  display: block;
  background: url('data:image/png;base64,') no-repeat;
  background-size: 22px;
  width: 22px;
  height: 22px;
  float: left;
  /* margin-left: 2px; */
}
th[sorthead].sortheaddesc:before {
  transform: rotate(180deg);
  -ms-transform: rotate(180deg); }
